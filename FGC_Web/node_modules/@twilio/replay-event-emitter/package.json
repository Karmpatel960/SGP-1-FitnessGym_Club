  "description": "An extension to Node's EventEmitter that supports event replay.",
  "main": "./builds/lib.js",
  "browser": "./builds/browser.js",
  "react-native": "./dist/index.js",
  "types": "./builds/lib.d.ts",
  "files": [
    "docs",
    "dist",
    "builds/**/*.d.ts",
    "builds/**/*.js",
    "builds/**/*.map",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
