    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.3",
    "prettier": "^2.3.0",
    "rollup": "^2.50.5",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2"
  },
  "browserslist": [
    "IE 11",
