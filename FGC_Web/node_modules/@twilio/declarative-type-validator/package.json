{
  "name": "@twilio/declarative-type-validator",
  "version": "0.1.11",
  "description": "Declarative runtime type validator for JavaScript",
  "main": "./builds/lib.js",
  "browser": "./builds/browser.js",
  "react-native": "./dist/index.js",
  "types": "./builds/lib.d.ts",
  "files": [
    "docs",
    "dist",
    "builds/**/*.d.ts",
    "builds/**/*.js",
    "builds/**/*.map",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf ./dist ./builds ./docs",
    "build": "npx rollup -c",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint src/**/*.ts --fix",
    "test:unit": "npx jest",
    "test:integration": "echo \"Integration tests don't exist for declarative-type-validator.\" && exit 0",
    "ci": "yarn clean && yarn lint && yarn build"
  },
  "author": "David Lysenko",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.14.5",
    "core-js": "^3.17.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.0",
    "rollup": "^2.51.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.0",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2"
  },
  "browserslist": [
    "IE 11",
    "last 3 Chrome versions",
    "last 3 Firefox versions",
    "last 3 Safari versions",
    "last 3 Edge versions",
    "last 2 iOS version",
    "last 2 ChromeAndroid version",
    "last 2 FirefoxAndroid version",
    "last 2 Samsung versions",
    "last 2 UCAndroid versions"
  ],
  "engines": {
    "node": ">=14"
  }
}
