{
  "name": "twilsock",
  "version": "0.12.2",
  "description": "Client library for TwilSock service",
  "main": "./builds/lib.js",
  "browser": "./builds/browser.js",
  "react-native": "./dist/index.js",
  "types": "./builds/lib.d.ts",
  "author": "Twilio",
  "license": "MIT",
  "files": [
    "docs",
    "dist",
    "builds/**/*.d.ts",
    "builds/**/*.js",
    "builds/**/*.map",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf ./coverage ./dist ./builds ./docs",
    "lint": "npx eslint src test --ext .ts",
    "lint:fix": "npx eslint src test --ext .ts --fix",
    "test:unit": "env FORCE_COLOR=1 NODE_ENV=test npx nyc --report-dir coverage/unit ./node_modules/.bin/mocha test/unit/index.ts",
    "test:integration": "env FORCE_COLOR=1 NODE_ENV=test npx nyc --report-dir coverage/integration ./node_modules/.bin/mocha test/integration/index.ts",
    "build": "npx rollup -c",
    "ssri": "yarn ssri:bundle && yarn ssri:min",
    "ssri:bundle": "openssl dgst -sha256 -binary ./builds/twilsock.js | openssl base64 -A | (printf \"sha256-\" && cat) | tee ./builds/twilsock.sri",
    "ssri:min": "openssl dgst -sha256 -binary ./builds/twilsock.min.js | openssl base64 -A | (printf \"sha256-\" && cat) | tee ./builds/twilsock.min.sri",
    "sourceMapReport:html": "npx source-map-explorer@2.5.2 ./builds/lib.js --html ./builds/lib.report.html --no-border-checks",
    "sourceMapReport:json": "npx source-map-explorer@2.5.2 ./builds/lib.js --json ./builds/lib.report.json --no-border-checks",
    "ci": "yarn clean && yarn lint && yarn build && yarn ssri && yarn sourceMapReport:html && yarn sourceMapReport:json"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.5",
    "@twilio/declarative-type-validator": "^0.1.11",
    "@twilio/operation-retrier": "^4.0.7",
    "core-js": "^3.17.3",
    "iso8601-duration": "=1.2.0",
    "javascript-state-machine": "^3.1.0",
    "loglevel": "^1.6.3",
    "platform": "^1.3.6",
    "uuid": "^3.4.0",
    "ws": "^5.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/loglevel": "^1.5.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.8",
    "@types/platform": "^1.3.2",
    "@types/sinon": "^4.3.1",
    "@types/sinon-chai": "^2.7.29",
    "@types/uuid": "^3.4.4",
    "@types/ws": "^7.4.6",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "async-test-tools": "^1.0.7",
    "better-docs": "^2.3.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cheerio": "^1.0.0-rc.2",
    "cross-spawn": "^7.0.3",
    "del": "^4.1.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "event-to-promise": "^0.8.0",
    "express": "^4.17.1",
    "glob": "^7.1.7",
    "http": "0.0.0",
    "http-proxy": "^1.18.1",
    "ink-docstrap": "^1.3.2",
    "isparta": "^4.1.1",
    "jsdoc": "^3.6.6",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "ngrok": "^3.2.5",
    "nyc": "^14.1.1",
    "prettier": "^2.3.2",
    "rollup": "^2.51.1",
    "rollup-plugin-polyfill-node": "^0.6.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.0",
    "run-sequence": "^2.2.1",
    "semver": "^5.5.0",
    "sinon": "^4.2.2",
    "sinon-chai": "^2.14.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=14"
  },
  "browserslist": [
    "IE 11",
    "last 3 Chrome versions",
    "last 3 Firefox versions",
    "last 3 Safari versions",
    "last 3 Edge versions",
    "last 2 iOS version",
    "last 2 ChromeAndroid version",
    "last 2 FirefoxAndroid version",
    "last 2 Samsung versions",
    "last 2 UCAndroid versions"
  ]
}
